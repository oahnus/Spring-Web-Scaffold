<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD com.example.Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "top.oahnus.mapper.primary.SysPermissionMapper">
    <resultMap id = "sysPermissionMap" type= "top.oahnus.domain.primary.SysPermission">
        <id property = "id" column = "id" javaType="Long" jdbcType="BIGINT"/>
        <result property = "name" column = "name" javaType="String"/>
        <result property = "desc" column = "desc" javaType="String"/>
        <result property = "permission" column = "permission" javaType="String"/>
        <result property = "enable" column = "enable" javaType="Boolean"/>
    </resultMap>

    <sql id="base">
        SELECT * FROM sys_permission
    </sql>
</mapper>